{
  "name": "mern-shop",
  "version": "1.0.0",
  "description": "ecom app made using MERN stack",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server \" \"npm run client",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarun-soni/mern-shop.git"
  },
  "keywords": [],
  "author": "Tarun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tarun-soni/mern-shop/issues"
  },
  "homepage": "https://github.com/tarun-soni/mern-shop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "engines": {
    "node": "14"
  }
}
